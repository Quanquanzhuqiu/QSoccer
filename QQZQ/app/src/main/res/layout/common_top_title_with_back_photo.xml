<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/topbar_root_ll">


    <LinearLayout style="@style/topbar_left_ll">

        <LinearLayout
            android:id="@+id/ll_back"
            style="@style/topbar_ll_back">

            <ImageView
                android:id="@+id/iv_back"
                style="@style/topbar_iv_back" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/tv_title"
        style="@style/topbar_tv_title"
        android:text="@string/team_gallery" />

    <LinearLayout
        android:id="@+id/ll_take_photo"
        style="@style/topbar_ll_more">

        <ImageView
            android:id="@+id/iv_commit"
            style="@style/topbar_iv_more"
            android:background="@drawable/ic_btn_take_photo" />

    </LinearLayout>
</LinearLayout>